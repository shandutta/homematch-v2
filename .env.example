# Google Maps API Configuration
# SECURITY: Use two separate API keys with different restrictions

# Server-side API key (Required for secure Maps integration)
# Configure with IP restrictions in Google Cloud Console
GOOGLE_MAPS_SERVER_API_KEY=your_server_restricted_api_key

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Optional: enable local Supabase proxying through Next.js for HTTPS dev setups
SUPABASE_LOCAL_PROXY=false
SUPABASE_LOCAL_PROXY_TARGET=http://127.0.0.1:54321

# Application URLs
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# External APIs
RAPIDAPI_KEY=your_rapidapi_key
RAPIDAPI_HOST=zillow-com1.p.rapidapi.com
ZILLOW_CRON_SECRET=your_random_cron_secret # used to authorize /api/admin/ingest/zillow
ZILLOW_LOCATIONS="San Francisco, CA;Oakland, CA;Berkeley, CA;San Jose, CA" # optional override list; defaults to Bay Area set in code
