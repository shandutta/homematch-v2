    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "express-rate-limit": "^8.0.1",
    "framer-motion": "^12.23.12",
    "inngest": "^3.40.1",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.525.0",
    "next-themes": "^0.4.6",
    "polygon-clipping": "^0.15.7",
    "posthog-js": "^1.258.2",
    "posthog-node": "^5.6.0",
    "rate-limiter-flexible": "^7.2.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.61.1",
    "react-hot-toast": "^2.5.2",
    "sonner": "^2.0.6",
