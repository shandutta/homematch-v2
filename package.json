{
  "name": "homematch-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpx kill-port 3000 && pnpx next dev --turbopack",
    "build": "pnpx next build",
    "start": "pnpx next start",
    "lint": "pnpx next lint",
    "type-check": "pnpx tsc --noEmit",
    "prepare": "simple-git-hooks",
    "format": "pnpx prettier --write .",
    "lint:fix": "pnpx eslint --fix .",
    "kill-node": "node scripts/kill-port.js",
    "pre-commit": "pnpm run format && pnpm run lint:fix && pnpm run type-check",
    "migrate": "pnpx tsx scripts/run-migration.ts",
    "migrate:schema": "echo 'Run the SQL file in Supabase SQL Editor: scripts/migrate/01-setup-schema.sql'",
    "format:file": "prettier --write --ignore-unknown && eslint --fix && tsc --noEmit",
    "test": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "test:unit": "jest",
    "test:integration": "pnpx tsx scripts/infrastructure-working.js start && pnpm exec vitest run",
    "test:integration:watch": "pnpx tsx scripts/infrastructure-working.js start && pnpm exec vitest",
    "build:test": "node scripts/build-for-tests.js",
    "test:e2e": "pnpx playwright test",
    "test:e2e:ui": "pnpx playwright test --ui",
    "test:e2e:headed": "pnpx playwright test --headed",
    "test:e2e:validate": "node scripts/validate-e2e-setup.js",
    "test:setup-users": "node scripts/setup-test-users-admin.js",
    "test:coverage": "pnpx jest --coverage",
    "test:unit:watch": "pnpx jest --watch",
    "test:db:reset": "pnpx tsx scripts/infrastructure-working.js reset-db",
    "test:cleanup": "pnpx tsx scripts/infrastructure-working.js stop-supabase && node scripts/clean-test-build.js",
    "test:infra:start": "pnpx tsx scripts/infrastructure-working.js start",
    "test:infra:restart": "pnpx tsx scripts/infrastructure-working.js restart",
    "test:infra:stop": "pnpx tsx scripts/infrastructure-working.js stop-supabase",
    "test:infra:health": "pnpx tsx scripts/infrastructure-working.js health",
    "validate:deployment": "pnpx tsx scripts/validate-deployment.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.23",
    "@hookform/resolvers": "^5.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@sentry/nextjs": "^9.42.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.52.1",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "ai": "^4.3.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "framer-motion": "^12.23.12",
    "inngest": "^3.40.1",
    "lucide-react": "^0.525.0",
    "next": "15.4.4",
    "next-themes": "^0.4.6",
    "posthog-js": "^1.258.2",
    "posthog-node": "^5.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.61.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm run format && pnpm run lint:fix && pnpm run type-check",
    "commit-msg": "npx commitlint --edit $1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^3.2.4",
    "dotenv": "^16.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "simple-git-hooks": "^2.13.0",
    "supabase": "^2.33.5",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
