{
  "name": "homematch-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:reset": "node scripts/dev-supabase-reset.js",
    "guard:supabase": "node scripts/guard-supabase-env.js",
    "ensure:docker": "node scripts/ensure-docker.js",
    "ensure:server-action-key": "node scripts/ensure-server-action-key.js",
    "dev": "pnpm run guard:supabase && pnpm run ensure:server-action-key && pnpm run ensure:docker && pnpm run db:reset && pnpm run test:setup-users && node scripts/kill-port.js 3000 && next dev --hostname 0.0.0.0 --port 3000",
    "dev:integration": "pnpm run guard:supabase && pnpm run ensure:server-action-key && node scripts/kill-port.js 3000 && next dev --hostname 0.0.0.0 --port 3000",
    "dev:warmup": "node scripts/dev-warmup.js",
    "build": "node scripts/safe-build.js",
    "analyze": "ANALYZE=true pnpm build",
    "clean": "node scripts/clean-next-cache.js",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "pnpm exec tsc --noEmit",
    "check": "pnpm run lint && pnpm run type-check",
    "prepare": "simple-git-hooks",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix .",
    "kill-node": "node scripts/kill-port.js",
    "pre-commit": "node scripts/pre-commit-hook.js",
    "migrate": "pnpm exec tsx scripts/run-migration.ts",
    "migrate:schema": "echo 'Run the SQL file in Supabase SQL Editor: scripts/migrate/01-setup-schema.sql'",
    "format:file": "prettier --write --ignore-unknown",
    "test": "concurrently -n unit,int -c blue,green \"pnpm test:unit\" \"pnpm test:integration\" && pnpm test:e2e",
    "test:unit": "cross-env NODE_ENV=test pnpm exec jest __tests__/unit/",
    "test:unit:debug": "cross-env NODE_ENV=test pnpm exec jest __tests__/unit/ --verbose --runInBand",
    "test:integration": "node scripts/run-integration-tests.js",
    "test:integration:watch": "pnpm exec tsx scripts/integration-test-setup.js && pnpm exec vitest",
    "build:test": "node scripts/build-for-tests.js",
    "test:e2e": "pnpm run test:e2e:ui-tests",
    "test:e2e:ui-tests": "cross-env NEXT_PUBLIC_TEST_MODE=true node scripts/playwright-wrapper.js test",
    "test:e2e:smoke": "node scripts/playwright-wrapper.js test __tests__/e2e/smoke-min.spec.ts",
    "test:e2e:ui": "node scripts/playwright-wrapper.js test --ui",
    "test:e2e:headed": "node scripts/playwright-wrapper.js test --headed",
    "test:e2e:validate": "node scripts/validate-e2e-setup.js",
    "test:setup-users": "node scripts/setup-test-users-admin.js",
    "test:coverage": "pnpm exec jest --coverage",
    "test:jest": "cross-env NODE_ENV=test pnpm exec jest",
    "test:unit:watch": "cross-env NODE_ENV=test pnpm exec jest __tests__/unit/ --watch",
    "test:db:reset": "pnpm exec tsx scripts/infrastructure-working.js reset-db",
    "test:cleanup": "pnpm exec tsx scripts/infrastructure-working.js stop-supabase && node scripts/clean-test-build.js",
    "test:infra:start": "pnpm exec tsx scripts/infrastructure-working.js start",
    "test:infra:restart": "pnpm exec tsx scripts/infrastructure-working.js restart",
    "test:infra:stop": "pnpm exec tsx scripts/infrastructure-working.js stop-supabase",
    "test:infra:health": "pnpm exec tsx scripts/infrastructure-working.js health",
    "validate:deployment": "pnpm exec tsx scripts/validate-deployment.js",
    "test:integration:remote": "node scripts/integration-remote.js",
    "test:e2e:remote:seed": "node scripts/remote-seed.js",
    "test:safety-net": "node __tests__/scripts/refactoring-safety-net.js",
    "test:safety-net:quick": "pnpm exec jest __tests__/integration/services/properties-integration.test.ts __tests__/integration/error-handling-patterns.test.ts",
    "test:refactoring-targets": "pnpm exec jest --collectCoverageFrom=\"src/lib/services/properties.ts\" --collectCoverageFrom=\"src/lib/services/interactions.ts\" --collectCoverageFrom=\"src/lib/supabase/server.ts\" --collectCoverageFrom=\"src/lib/supabase/client.ts\" --coverage",
    "test:filter-patterns": "pnpm exec jest __tests__/integration/filter-builder-patterns.test.ts --verbose",
    "test:supabase-patterns": "pnpm exec jest __tests__/integration/supabase-client-patterns.test.ts --verbose",
    "test:e2e:remote": "node scripts/e2e-remote.js",
    "dev:remote": "node scripts/dev-remote.js",
    "ci:setup": "chmod +x scripts/ci-test-setup.sh && ./scripts/ci-test-setup.sh",
    "ci:test:integration": "cross-env NODE_ENV=test pnpm exec vitest run --config vitest.ci.config.ts",
    "ci:test:database": "cross-env NODE_ENV=test pnpm exec vitest run __tests__/integration/database/",
    "ci:test:api": "cross-env NODE_ENV=test pnpm exec vitest run __tests__/integration/api/",
    "ci:validate:schema": "node scripts/validate-db-schema.js",
    "ci:validate:rls": "node scripts/test-rls-policies.js",
    "ci:health": "node scripts/health-check.js",
    "auto:commit": "node scripts/auto-commit.js",
    "ingest:zillow": "pnpm exec tsx scripts/ingest-zillow.ts",
    "cleanup:properties:ba": "pnpm exec tsx scripts/cleanup-properties-bayarea.ts",
    "refresh:zillow-status": "pnpm exec tsx scripts/refresh-zillow-status.ts",
    "refresh:zillow-status:search": "pnpm exec tsx scripts/refresh-zillow-status-search.ts",
    "refresh:zillow-status:detail": "pnpm exec tsx scripts/refresh-zillow-status-detail.ts",
    "normalize:cities": "pnpm exec tsx scripts/normalize-cities.ts",
    "report:properties": "pnpm exec tsx scripts/report-properties.ts",
    "report:zillow-coverage": "pnpm exec tsx scripts/report-zillow-coverage.ts",
    "cron:zombie-watch": "node scripts/zombie-watch.js",
    "cron:nightly-shutdown": "node scripts/nightly-shutdown.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.23",
