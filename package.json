    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^26.1.0",
    "next": "15.4.5",
    "node-fetch": "^2.7.0",
    "node-mocks-http": "^1.17.2",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "psql": "^0.0.1",
    "simple-git-hooks": "^2.13.0",
